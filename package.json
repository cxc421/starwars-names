{
  "name": "@chrischuang/starwars-names",
  "version": "0.0.0-development",
  "description": "Get random Star Wars names",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --ignore \"src/**/*.test.js\"",
    "postbuild": "babel src/starwars-names.json --out-dir dist --copy-files",
    "watch:test": "npm t -- -w",
    "test": "mocha src/index.test.js --require @babel/register",
    "cover": "nyc npm t",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cxc421/starwars-names.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "Chris Chuang <chrischuang0421@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cxc421/starwars-names/issues"
  },
  "homepage": "https://github.com/cxc421/starwars-names#readme",
  "dependencies": {
    "unique-random-array": "2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@babel/register": "7.7.7",
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "ghooks": "2.0.4",
    "mocha": "6.2.2",
    "nyc": "15.0.0",
    "rimraf": "3.0.0",
    "semantic-release": "15.14.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
